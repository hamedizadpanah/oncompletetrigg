    <validationRules>
        <fullName>IndividualOrOrg</fullName>
        <active>true</active>
        <description>Validates that only Individual or Organization is populated.</description>
        <errorConditionFormula>NOT(ISBLANK(Constituent__c)) &amp;&amp; NOT(ISBLANK( Organization__c))</errorConditionFormula>
        <errorMessage>A gift cannot be associated to both an Individual and an Organization.  Only one of the fields can be populated per gift.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IndividualOrOrgRequired</fullName>
        <active>true</active>
        <description>Displays an error if Transaction is not hard credited to either an Individual or Organization</description>
        <errorConditionFormula>AND(ISBLANK( Constituent__c ),ISBLANK( Organization__c ))</errorConditionFormula>
        <errorMessage>You must hard credit the transaction to either an Individual or an Organizations</errorMessage>
    </validationRules>
